---
import Layout from '../layouts/Layout.astro';
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';

const theme = Astro.cookies.get('theme')?.value || 'dark';
const textColor = theme === 'dark' ? 'text-primary' : 'text-primary';
---

<Layout title="Sergiy Dybskiy, Builder">
  <div class="!visible transition-opacity duration-150 bg-background text-foreground !opacity-100" id="v0-container" style="visibility:hidden;opacity:0">
    <div class="min-h-screen font-mono p-4 flex items-center justify-center transition-colors duration-0" style="font-size: 14px;">
      <main class="max-w-[700px] w-full">
        <Header name="sergiy dybskiy" location="toronto, canada" />

        <section class="mb-8" style="opacity: 1; will-change: opacity;">
          <div class="flex items-center mb-2">
            <svg fill="none" height="10" viewBox="0 0 36 18" width="20" xmlns="http://www.w3.org/2000/svg">
              <g fill="currentColor">
                <path d="m12.7038 0c.2486 0 .45.201472.45.45v3.25385c0 .24852-.2014.45-.45.45h-3.7038c-2.67646 0-4.84615 2.16969-4.84615 4.84615 0 2.6765 2.16969 4.8462 4.84615 4.8462h3.7038c.2486 0 .45.2014.45.45v3.2538c0 .2485-.2014.45-.45.45h-3.7038c-4.97056 0-9-4.0294-9-9 0-4.97056 4.02944-9 9-9z"/>
                <path d="m27 4.15385h-3.7038c-.2486 0-.45-.20148-.45-.45v-3.25385c0-.248528.2014-.45.45-.45h3.7038c4.9706 0 9 4.02944 9 9 0 4.9706-4.0294 9-9 9h-3.7038c-.2486 0-.45-.2015-.45-.45v-3.2538c0-.2486.2014-.45.45-.45h3.7038c2.6765 0 4.8462-2.1697 4.8462-4.8462 0-2.67646-2.1697-4.84615-4.8462-4.84615z"/>
                <path d="m20.0769.45c0-.248528-.2014-.45-.45-.45h-3.2538c-.2486 0-.45.201472-.45.45v6.02308c0 .24852-.2015.45-.45.45h-6.4731c-1.14705 0-2.07692.92987-2.07692 2.07692 0 1.1471.92987 2.0769 2.07692 2.0769h6.4731c.2485 0 .45.2015.45.45v6.0231c0 .2485.2014.45.45.45h3.2538c.2486 0 .45-.2015.45-.45v-6.0231c0-.2485.2015-.45.45-.45h6.4731c1.1471 0 2.0769-.9298 2.0769-2.0769 0-1.14705-.9299-2.07692-2.0769-2.07692h-6.4731c-.2485 0-.45-.20148-.45-.45z"/>
              </g>
            </svg>
            <span class={`${textColor} ml-2`}>Acctual <span class="text-muted">Founding Engineer</span></span>
          </div>
          <p class={textColor}>Founding Engineer at Acctual. Passionate about creating seamless user experiences and pushing the boundaries of digital interfaces.</p>
        </section>

        <section class="mb-8" style="opacity: 1; will-change: opacity;">
          <h2 class="font-semibold mb-2">interests</h2>
          <ul class="space-y-1">
            <li class="flex items-center">
              <span class="mr-2 text-muted">-</span>
              <span class={textColor}>User Interface Design</span>
            </li>
            <li class="flex items-center">
              <span class="mr-2 text-muted">-</span>
              <span class={textColor}>User Experience</span>
            </li>
            <li class="flex items-center">
              <span class="mr-2 text-muted">-</span>
              <span class={textColor}>Design Systems</span>
            </li>
            <li class="flex items-center">
              <span class="mr-2 text-muted">-</span>
              <span class={textColor}>Front-end Development</span>
            </li>
            <li class="flex items-center">
              <span class="mr-2 text-muted">-</span>
              <span class={textColor}>Open Source</span>
            </li>
          </ul>
        </section>

        <section class="mb-8" style="opacity: 1; will-change: opacity;">
          <h2 class="font-semibold mb-2">previously</h2>
          <ul class="space-y-2">
            <li class="flex items-center">
              <div class="w-6 h-6 mr-2 flex items-center justify-center">
                <svg aria-label="Coinbase logo" height="16" role="img" viewBox="0 0 48 48" width="16" xmlns="http://www.w3.org/2000/svg"><title>Coinbase logo</title><path d="M24,36c-6.63,0-12-5.37-12-12s5.37-12,12-12c5.94,0,10.87,4.33,11.82,10h12.09C46.89,9.68,36.58,0,24,0 C10.75,0,0,10.75,0,24s10.75,24,24,24c12.58,0,22.89-9.68,23.91-22H35.82C34.87,31.67,29.94,36,24,36z" fill="currentColor"></path></svg>
              </div>
              <span class={textColor}>Coinbase <span class="text-muted">Senior Frontend Developer</span></span>
            </li>
            <li class="flex items-center">
              <div class="w-6 h-6 mr-2 flex items-center justify-center">
                <svg version="1.1" width="20" height="20" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 109.5 124.5">
                  <g>
                    <path d="M74.7,14.8c0,0-1.4,0.4-3.7,1.1c-0.4-1.3-1-2.8-1.8-4.4c-2.6-5-6.5-7.7-11.1-7.7c0,0,0,0,0,0c-0.3,0-0.6,0-1,0.1 c-0.1-0.2-0.3-0.3-0.4-0.5c-2-2.2-4.6-3.2-7.7-3.1c-6,0.2-12,4.5-16.8,12.2c-3.4,5.4-6,12.2-6.7,17.5c-6.9,2.1-11.7,3.6-11.8,3.7 c-3.5,1.1-3.6,1.2-4,4.5C9.1,40.7,0,111.2,0,111.2l75.6,13.1V14.6C75.2,14.7,74.9,14.7,74.7,14.8z M57.2,20.2 c-4,1.2-8.4,2.6-12.7,3.9c1.2-4.7,3.6-9.4,6.4-12.5c1.1-1.1,2.6-2.4,4.3-3.2C56.9,12,57.3,16.9,57.2,20.2z M49.1,4.3 c1.4,0,2.6,0.3,3.6,0.9c-1.6,0.8-3.2,2.1-4.7,3.6c-3.8,4.1-6.7,10.5-7.9,16.6c-3.6,1.1-7.2,2.2-10.5,3.2 C31.7,19.1,39.8,4.6,49.1,4.3z M37.4,59.3c0.4,6.4,17.3,7.8,18.3,22.9c0.7,11.9-6.3,20-16.4,20.6c-12.2,0.8-18.9-6.4-18.9-6.4 l2.6-11c0,0,6.7,5.1,12.1,4.7c3.5-0.2,4.8-3.1,4.7-5.1c-0.5-8.4-14.3-7.9-15.2-21.7C23.8,51.8,31.4,40.1,48.2,39 c6.5-0.4,9.8,1.2,9.8,1.2l-3.8,14.4c0,0-4.3-2-9.4-1.6C37.4,53.5,37.3,58.2,37.4,59.3z M61.2,19c0-3-0.4-7.3-1.8-10.9 c4.6,0.9,6.8,6,7.8,9.1C65.4,17.7,63.4,18.3,61.2,19z" fill="currentColor"/>
                    <path d="M78.1,123.9l31.4-7.8c0,0-13.5-91.3-13.6-91.9c-0.1-0.6-0.6-1-1.1-1c-0.5,0-9.3-0.2-9.3-0.2s-5.4-5.2-7.4-7.2V123.9z" fill="currentColor"/>
                  </g>
                </svg>
              </div>
              <span class={textColor}>Shopify <span class="text-muted">Senior Frontend Developer</span></span>
            </li>
            <li class="flex items-center">
              <span class={textColor}>Ruutly (acquired by <a href="https://betakit.com/careerbeacon-acquires-employer-branding-platform-provider-ruutly/" target="_blank" class={textColor}>CareerBeacon</a>) <span class="text-muted">CTO / Co-Founder</span></span>
            </li>
          </ul>
        </section>

        <section class="mb-8" style="opacity: 1; will-change: opacity;">
          <h2 class="font-semibold mb-2">hobbies</h2>
          <div class="grid grid-cols-2 gap-3 group hover-group">
            <a href="#" class="border p-1 border-muted hover:border-primary transition-all duration-300 ease-in-out opacity-100 hover-group-hover:opacity-50 hover:!opacity-100 hover:!border-opacity-100">
              <span class="flex items-center justify-between {textColor}">Tennis</span>
            </a>
            <a href="#" class="border p-1 border-muted hover:border-primary transition-all duration-300 ease-in-out opacity-100 hover-group-hover:opacity-50 hover:!opacity-100 hover:!border-opacity-100">
              <span class="flex items-center justify-between {textColor}">Snowboarding</span>
            </a>
            <a href="#" class="border p-1 border-muted hover:border-primary transition-all duration-300 ease-in-out opacity-100 hover-group-hover:opacity-50 hover:!opacity-100 hover:!border-opacity-100">
              <span class="flex items-center justify-between {textColor}">Lego</span>
            </a>
            <a href="#" class="border p-1 border-muted hover:border-primary transition-all duration-300 ease-in-out opacity-100 hover-group-hover:opacity-50 hover:!opacity-100 hover:!border-opacity-100">
              <span class="flex items-center justify-between {textColor}">Travel</span>
            </a>
          </div>
        </section>

        <section class="mb-8" style="opacity: 1; will-change: opacity;">
          <h2 class="font-semibold mb-2">community</h2>
          <ul class="space-y-2">
            <li class="flex items-center">
              <span class="mr-2 text-muted">-</span>
              <span class={textColor}><a href="https://guild.host/react-toronto" class={textColor}>React Toronto</a> <span class="text-muted">Organizer</span></span>
            </li>
          </ul>
        </section>

        <section class="mb-8" style="opacity: 1; will-change: opacity;">
          <h2 class="font-semibold mb-2">demos</h2>
          <ul class="space-y-2">
            <li class="flex items-center">
              <span class="mr-2 text-muted">-</span>
              <span class={textColor}><a href="/error-prediction" class={textColor}>Sentry Error Prediction</a> <span class="text-muted">AI-powered error detection showcase</span></span>
            </li>
          </ul>
        </section>

        <Footer />
      </main>
    </div>
  </div>
</Layout>
